    {
      "name": "Auth",
      "item": [
        {
          "name": "Login",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if(pm.response.code === 200) {",
                  "  var response = pm.response.json();",
                  "  if(response.data && response.data.token) {",
                  "    pm.environment.set('token', response.data.token);",
                  "    pm.environment.set('userId', response.data.user.id);",
                  "    pm.environment.set('userRole', response.data.user.role);",
                  "    pm.environment.set('userStore', response.data.user.storeId);",
                  "    console.log('✅ Token guardado:', response.data.token.substring(0, 20) + '...');",
                  "    console.log('✅ Usuario:', response.data.user.email);",
                  "  } else {",
                  "    console.log('❌ Token no encontrado en response');",
                  "    console.log('Response:', JSON.stringify(response, null, 2));",
                  "  }",
                  "}",
                  "pm.test('Status code es 200', function () {",
                  "    pm.response.to.have.status(200);",
                  "});",
                  "pm.test('Response tiene token', function () {",
                  "    var response = pm.response.json();",
                  "    pm.expect(response.data).to.have.property('token');",
                  "});"
                ],
                "type": "text/javascript"
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"email\": \"admin@example.com\",\n  \"password\": \"password123\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/api/auth/login",
              "host": ["{{baseUrl}}"],
              "path": ["api", "auth", "login"]
            }
          }
