{
  "name": "hot-wheels-manager-backend",
  "version": "1.0.0",
  "description": "Backend API for Hot Wheels Manager",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:nodemon": "nodemon --exec tsx src/index.ts --ext ts --watch src",
    "build": "mkdir -p src/shared && cp -r ../shared/* src/shared/ 2>/dev/null || true && npx -p typescript tsc",
    "start": "node dist/index.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "seed": "tsx src/scripts/seedDatabase.ts",
    "seed:locations": "tsx src/scripts/seedDeliveryLocations.ts",
    "migrate:presale-statuses": "tsx src/scripts/migrate-presale-statuses.ts",
    "migrate:add-fantasy": "tsx src/scripts/add-fantasy-field.ts",
    "reset-db": "tsx src/scripts/reset-database.ts",
    "create-admin": "tsx src/scripts/create-admin-interactive.ts",
    "check-users": "tsx src/scripts/check-users.ts",
    "migrate:recreate-deliveries": "tsx src/scripts/recreateCompletedDeliveries.ts",
    "migrate:sales-snapshot": "tsx src/scripts/migrateSalesSnapshot.ts",
    "fix:dates:preview": "tsx src/scripts/fixDateMigration.ts --preview",
    "fix:dates": "tsx src/scripts/fixDateMigration.ts --fix",
    "fandom-scraper": "tsx src/scripts/fandom-scraper.ts",
    "complete-scraper": "tsx src/scripts/complete-scraper.ts",
    "scrape:all": "tsx src/scripts/scrape-all-categories.ts",
    "scrape:intelligent": "tsx src/scripts/scrape-intelligent.ts",
    "update-pack-photos": "tsx src/scripts/update-pack-photos.ts",
    "update-pack-items-photos": "tsx src/scripts/update-pack-items-photos.ts",
    "clean-photo-urls": "tsx src/scripts/clean-photo-urls.ts",
    "decode-photo-urls": "tsx src/scripts/decode-photo-urls.ts",
    "reencode-photo-urls": "tsx src/scripts/reencode-photo-urls.ts",
    "fix-incomplete-urls": "tsx src/scripts/fix-incomplete-urls.ts",
    "fix-incomplete-urls-v2": "tsx src/scripts/fix-incomplete-urls-v2.ts",
    "sync-to-mongo": "tsx src/scripts/sync-to-mongo.ts",
    "backfill-images": "tsx src/scripts/backfill-images.ts",
    "export-photos-json": "tsx src/scripts/export-photos-to-json.ts",
    "fix-truncated-photos": "tsx src/scripts/fix-truncated-photos.ts",
    "fix-shared-photos": "tsx src/scripts/fix-shared-photos.ts",
    "classify-segments": "tsx src/scripts/classify-segments.ts",
    "scrape:amazon": "tsx src/scripts/scrape-amazon-premium-brands.ts",
    "scrape:premium": "tsx src/scripts/scrape-premium-brands.ts"
  },
  "keywords": [
    "hot-wheels",
    "api",
    "express",
    "mongodb"
  ],
  "author": "Antonio",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@types/cheerio": "^0.22.35",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.2.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "form-data": "^4.0.4",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "undici": "~5.20.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "mongodb-memory-server": "^10.2.1",
    "nodemon": "^3.1.11",
    "tsx": "^4.6.2",
    "typescript": "^5.9.3"
  }
}
